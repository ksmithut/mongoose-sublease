{
  "name": "mongoose-sublease",
  "version": "0.5.0",
  "description": "A library to help make database-level multitenancy in mongoose easy",
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "jest",
    "coverage.report": "open coverage/lcov-report/index.html",
    "codeclimate": "cat ./coverage/lcov.info | codeclimate-test-reporter",
    "format": "prettier-eslint src/**/*.js --write",
    "lint": "eslint src/**/*.js && prettier-eslint src/**/*.js --list-different"
  },
  "engine": {
    "node": ">= 12"
  },
  "files": [
    "index.js",
    "express.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksmithut/mongoose-sublease.git"
  },
  "keywords": [
    "mongoose",
    "multitenancy",
    "tenant"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/mongoose-sublease/issues"
  },
  "homepage": "https://github.com/ksmithut/mongoose-sublease#readme",
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "dependencies": {},
  "peerDependencies": {
    "mongoose": "4.x || 5.x"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "jest": "^26.1.0",
    "mongodb-memory-server": "^6.6.1",
    "mongoose": "5.9.25",
    "prettier-eslint-cli": "^5.0.0",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  }
}
